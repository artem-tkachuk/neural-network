from math import exp
def sigmoid(x, derivative=False):
    sigm = 1.0 / (1 + exp(-x))
    return  sigm * (1.0 - sigm) if derivative else sigm
one = sigmoid(1)
two = sigmoid(2)
three = sigmoid(3)
l = np.array([[one + one], [two + two], [two + two], [three + three]])
